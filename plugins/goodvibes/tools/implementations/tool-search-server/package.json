{
  "name": "goodvibes-tool-search-server",
  "version": "1.0.0",
  "description": "MCP server for searching GoodVibes plugin registries",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.js --external:node:*",
    "dev": "tsc --watch",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "fuse.js": "latest",
    "js-yaml": "latest"
  },
  "devDependencies": {
    "@types/js-yaml": "latest",
    "@types/node": "latest",
    "typescript": "latest",
    "esbuild": "latest"
  }
}
